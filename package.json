  "bugs": {
    "url": "https://github.com/holiday-jp/holiday_jp-js/issues"
  },
  "homepage": "https://github.com/holiday-jp/holiday_jp-js",
  "devDependencies": {
    "@playwright/test": "^1.48.0",
    "js-yaml": "^3.13.1",
    "moment": "^2.23.0",
    "uglify-js": "^3.19.3",
    "webpack": "^5.91.0",
    "webpack-cli": "^4.2.0"
  }
}
